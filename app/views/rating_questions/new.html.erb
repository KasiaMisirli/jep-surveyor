<div class="form">
  <%= form_with model: @rating_question, url: rating_questions_path,local: true do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :tag %>
  <%= f.text_field :tag %>
  <%= f.submit %>
  <% end %>
<%= link_to 'Back', rating_questions_path %>
  <div class='questions'>
    <h4><%= @rating_question.title %></h4>
    <h4><%= @rating_question.tag %></h4>
  </div>
</div>
