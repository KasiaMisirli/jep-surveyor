
<div class="form">
  <%= form_with model: @rating_question, url: rating_question_path,local: true do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :tag %>
  <%= f.text_field :tag %>
  <%= f.submit %>
  <% end %>
  <%= link_to 'Back', rating_questions_path %>
  <div class="single">
    <div class='questions'>
      <% if @rating_question %>
        <h4>Previous title: <%= @rating_question.title %></h4>
        <h4>Previous tag: <%= @rating_question.tag %></h4>
      <% end %>
    </div>
  </div>
</div>
